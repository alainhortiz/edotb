<tbody id="tb_body_replace">
    {% for dato in datos %}
        <tr>
            <td colspan="3" style="font-weight: bold">{{ dato['factor'] }}</td>
            {% for paciente in dato['pacientes'] %}
                <td {{ loop.last == true ? 'colspan="2"' }} class="text-center" >{{ paciente }}</td>
            {% endfor %}
        </tr>
    {% endfor %}
</tbody>